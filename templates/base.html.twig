<!DOCTYPE html>
<html lang="fr">
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Metallian Shop | Merchandising Musique, métal et punk{% endblock %}</title>
        <meta name="description" content="Metallian Eshop affilié à METALLIAN Magazine - nouveau store pour acheter du merchandising Musique et Pop Culture | stock de plus de 8000 articles">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <link rel="canonical" href="https://www.ornchanarong.com/" />
        <link rel="icon" href="{{ asset('static_images/logo_svg.svg') }}">
        <script src="{{ asset('jquery-3.6.1.js') }}"></script>
        {% block stylesheets %}
        <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-iYQeCzEYFbKjA/T2uDLTpkwGzCiq6soy8tYaI1GyVh/UjpbCx/TYkiZhlZB6+fzT" crossorigin="anonymous">
        <link rel="stylesheet"
        href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0-beta3/css/all.min.css">
        {{ encore_entry_link_tags('app') }}
        {% endblock %}

    </head>
    <body>
   
        {% include "include/_nav.html.twig" %}

        {% block body %}
        
        {% endblock %}

        {% include "include/_footer.html.twig" %}

        {% block javascripts %}
        <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.1/dist/js/bootstrap.bundle.min.js" integrity="sha384-u1OknCvxWvY5kfmNBILK2hRnQC3Pr17a+RTT6rIHI7NnikvbZlHgTPOOmMi466C8" crossorigin="anonymous"></script>
        <script src="https://cdn.jsdelivr.net/npm/bs5-lightbox@1.8.2/dist/index.bundle.min.js" defer></script>
        {{ encore_entry_script_tags('app') }}
        <script src="{{ asset('customJs.js') }}"></script>
        {% endblock %}
    </body>
</html>
